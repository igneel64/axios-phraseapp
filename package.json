{
  "name": "axios-phraseapp",
  "version": "0.0.2",
  "description": "Axios package to provide useful information and functionality about interactions with the PhraseApp API",
  "main": "./dist/axios-phraseapp.js",
  "scripts": {
    "test": "jest",
    "prebuild": "npm test && rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "postbuild": "bundlesize",
    "prerelease": "npm run build",
    "release": "npm version -m \"Released %s !\"",
    "postrelease": "npm run push && npm publish",
    "push": "git push origin master && git push origin --tags"
  },
  "keywords": [
    "axios",
    "phraseapp",
    "interceptors"
  ],
  "files": [
    "dist"
  ],
  "typings": "./lib/types.d.ts",
  "author": {
    "name": "Peter Perlepes",
    "email": "p.perlepes@gmail.com"
  },
  "license": "ISC",
  "peerDependencies": {
    "axios": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@types/jest": "^25.1.3",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "bundlesize": "^0.18.0",
    "cross-env": "^7.0.0",
    "jest": "^25.1.0",
    "nock": "^12.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "@babel/runtime": "^7.8.4"
  },
  "bundlesize": [
    {
      "path": "./dist/axios-phraseapp.js",
      "maxSize": "1.2 kB"
    }
  ],
  "jest": {
    "testEnvironment": "node"
  }
}
